<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>CaseModel</servlet-name>
        <servlet-class>CaseModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersModel</servlet-name>
        <servlet-class>UsersModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SchoolModel</servlet-name>
        <servlet-class>SchoolModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChildrenModel</servlet-name>
        <servlet-class>Children.ChildrenModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChildModel</servlet-name>
        <servlet-class>ChildModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FosterFamilyModel</servlet-name>
        <servlet-class>FosterFamilyModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RiskModel</servlet-name>
        <servlet-class>RiskModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbuseReportModel</servlet-name>
        <servlet-class>AbuseReportModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocumentModel</servlet-name>
        <servlet-class>DocumentModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SupportRequestsModel</servlet-name>
        <servlet-class>SupportRequestsModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LegalSupportModel</servlet-name>
        <servlet-class>LegalSupportModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginModel</servlet-name>
        <servlet-class>LoginModel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CaseModel</servlet-name>
        <url-pattern>/CaseModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsersModel</servlet-name>
        <url-pattern>/UsersModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SchoolModel</servlet-name>
        <url-pattern>/SchoolModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChildModel</servlet-name>
        <url-pattern>/ChildModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FosterFamilyModel</servlet-name>
        <url-pattern>/FosterFamilyModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RiskModel</servlet-name>
        <url-pattern>/RiskModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbuseReportModel</servlet-name>
        <url-pattern>/AbuseReportModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocumentModel</servlet-name>
        <url-pattern>/DocumentModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SupportRequestsModel</servlet-name>
        <url-pattern>/SupportRequestsModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LegalSupportModel</servlet-name>
        <url-pattern>/LegalSupportModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginModel</servlet-name>
        <url-pattern>/LoginModel</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
